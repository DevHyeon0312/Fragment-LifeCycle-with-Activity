<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.devhyeon.fragmentlifecycle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FragmentLifeCycle">
        <activity android:name=".activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activitys.ATypeActivity"/>
        <activity android:name=".activitys.BTypeActivity"/>
        <activity android:name=".activitys.CTypeActivity"/>
        <activity android:name=".activitys.DTypeActivity"/>

        <activity android:name=".activitys.MyDialogActivity"
            android:theme="@android:style/Theme.Holo.Light.Dialog"/>

    </application>

</manifest>